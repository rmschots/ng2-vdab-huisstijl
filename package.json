    "inline": "ng2inline -o tmp -c true \"lib/**/*.ts\"",
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/vdabhuisstijl.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/vdabhuisstijl.umd.min.js",
